a,b,c,d,e = 0,0,0,0,0

for each in stats:    
    if int(each) == 1:
        a += 1
    if int(each) == 2:
        b += 1
    if int(each) == 3:
        c += 1
    if int(each) == 4:
        d += 1
    if int(each) == 5:
        e += 1


print "\n"
print len(stats)
print str("%.2f" % (float(a)/len(stats)*100)) + "% of the chosen was 1."
print str("%.2f" % (float(b)/len(stats)*100)) + "% of the chosen was 2."
print str("%.2f" % (float(c)/len(stats)*100)) + "% of the chosen was 3."
print str("%.2f" % (float(d)/len(stats)*100)) + "% of the chosen was 4."
print str("%.2f" % (float(e)/len(stats)*100)) + "% of the chosen was 5."


print "\n"

print "Winings: " + str(winnings)
print "Loses: " + str(loses)
print "Pocketbook: " + str(user_credits)

