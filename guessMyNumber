import random
mynumber = random.randrange(1,6)
#print mynumber

user_credits = 10000
dubdown = 'Y'

stats =[]

print "\nYou have: " + str(user_credits) + " user credits."
placebet = 2
winnings = 0
loses = 0
stats.append(mynumber)

looper = 0

while looper < 999:
#while dubdown == 'Y':
    
##        if user_credits == 0:
##            print "Get yo broke ass outta here!"
##            print "\nYou lost a total of " + str(loses) +" credits."
##            print "And won " + str(winnings) + " credits. (" + str(int(winnings - loses)) +")"
##            break
##        
    #placebet = int(raw_input ("\nPlace your bet ["+str(placebet)+"]: ") or placebet)
    placebet = 2
    mynumber = random.randrange(1,6)
    looper += 1
    stats.append(mynumber)
##
##        if user_credits < placebet:
##            print "You can't afford to bet that! Try again, foo!"
##            placebet = raw_input ("Place your bet: ") or user_credits
##        
##        myguess = raw_input("\nGuess my number, pick a number between 1 and 10: ") or 0

##        while int(myguess) <=0 or int(myguess) > 5:
##            print "Take a damn VALID guess!"
##            myguess = raw_input("\nGuess my number, pick a number between 1 and 10: ") or 0

    ##if int(myguess) == mynumber:
    if random.randrange(1,6) == mynumber:
        winnings += int(placebet)
        
        #print "\nThat was a lucky guess\n"
        user_credits += int(placebet)

        #print "You won " + str(placebet) + " credits which means you have " + str(user_credits)
        #dubdown = raw_input("Another bet? [Y/N]: ").upper() or "Y"
        #dubdown = "Y"
        
        #mynumber = random.randrange(1,6)
        #print mynumber


    ##elif int(myguess) <> mynumber:
    elif random.randrange(1,6) <> mynumber:
        loses += int(placebet)
        #print "HAHA! Sucka! Try again!\n"
        user_credits -= int(placebet)


        #print "You lost! Now you have " + str(user_credits) + " left...SUCKA!."
        #dubdown = raw_input("Another bet? [Y/N]: ").upper() or "Y"
        #dudown = "Y"


    
